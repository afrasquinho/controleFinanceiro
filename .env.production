# Production Environment Variables
NODE_ENV=production

# Bundle Analyzer
GENERATE_SOURCEMAP=false
BUILD_PATH=build

# Performance optimizations
REACT_APP_BUNDLE_ANALYZE=true
REACT_APP_COMPRESS_ASSETS=true

# Firebase (use your production keys)
REACT_APP_FIREBASE_API_KEY=your_production_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_production_domain
REACT_APP_FIREBASE_PROJECT_ID=your_production_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_production_storage
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_production_sender_id
REACT_APP_FIREBASE_APP_ID=your_production_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_production_measurement_id
